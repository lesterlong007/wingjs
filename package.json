{
  "name": "wingjs",
  "version": "1.0.0",
  "description": "A utility and elegant javascript library",
  "main": "dist/umd/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix --ext .js,.ts, src/",
    "prettier": "prettier --write ./src/**/**/*.ts",
    "build": "rollup --config scripts/rollup.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js.jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js.jsx,ts,tsx}": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lesterlong007/wingjs.git"
  },
  "keywords": [
    "wing",
    "wingjs",
    "lester",
    "javascript",
    "utility"
  ],
  "author": "Lester",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lesterlong007/wingjs/issues"
  },
  "homepage": "https://github.com/lesterlong007/wingjs#readme",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
